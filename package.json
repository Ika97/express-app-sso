{
  "name": "sr-sso",
  "version": "0.0.0",
  "author": "Stansberry Research Web Team",
  "description": "A Single Sign-On application for Stansberry Research",
  "license": "ISC",
  "homepage": "https://github.com/Stansberry-Holdings/Stansberry-Research-SSO#README",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stansberry-Holdings/Stansberry-Research-SSO.git"
  },
  "keywords": [
    "Stansberry Research",
    "SSO",
    "Login"
  ],
  "bugs": {
    "url": "http://jira.cloudsna.com:8080/secure/RapidBoard.jspa?rapidView=117"
  },
  "main": "app.js",
  "scripts": {
    "debug": "npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/app.js",
    "start": "node dist/app.js",
    "test": "mocha \"test/*.test.ts\" --require ts-node/register --reporter spec --bail false --exit",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/app.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "connect-redis": "3.3.3",

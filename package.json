    "express": "4.16.3",
    "express-flash": "0.0.2",
    "express-session": "1.15.6",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "uuid": "3.3.2",
    "validator": "10.4.0",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/compression": "0.0.36",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/csurf": "1.9.35",
    "@types/debug": "0.0.30",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.10",
